<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.babel.kliky">



    <permission android:name="com.babel.kliky.provider.READWRITE" />
    <uses-permission android:name="com.babel.kliky.provider.READWRITE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name='android.permission.WAKE_LOCK'/>



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CounterActivity" />
        <activity android:name=".MyPreferencesActivity" />
        <receiver android:name='com.babel.kliky.AlarmReceiver'>
        </receiver>



    </application>

</manifest>